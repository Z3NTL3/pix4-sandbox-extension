{
    "manifest_version": 3,
    "name": "Code SandBox Sharer",
    "description": "Share your code with a nice dark hightlighting on code.pix4.dev",
    "version": "1.0",
    "default_locale": "en",
    
    "action": {
      "default_popup": "index.html",
      "default_icon": "img/logo.png"
    },
    "icons": {
      "128": "img/logo.png"
    },
    "permissions": ["tabs"]
  }